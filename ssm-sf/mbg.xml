<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>

  <context id="DB2Tables" targetRuntime="MyBatis3">
  	<commentGenerator>
  <property name="suppressAllComments" value="true" />
  <!--数据库名 -->
</commentGenerator>
    <jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://localhost:3306/ruserver" userId="root" password="zqqlx216">
    </jdbcConnection>

    <javaTypeResolver >
      <property name="forceBigDecimals" value="false" />
    </javaTypeResolver>
<!-- 指定javaBean生产位置 -->
    <javaModelGenerator targetPackage="com.sf.bean" targetProject=".\src\main\java">
      <property name="enableSubPackages" value="true" />
      <property name="trimStrings" value="true" />
    </javaModelGenerator>
<!-- 指定sql映射文件生成位置-->
    <sqlMapGenerator targetPackage="mapper"  targetProject=".\src\main\resources">
      <property name="enableSubPackages" value="true" />
    </sqlMapGenerator>
<!-- 指定dao接口生成的位置,mapper接口-->
    <javaClientGenerator type="XMLMAPPER" targetPackage="com.sf.dao"  targetProject=".\src\main\java">
      	<property name="enableSubPackages" value="true" />
    </javaClientGenerator>
<!--指定的表 -->
    <table  tableName="user_order" domainObjectName="User"  enableCountByExample="false"
            enableUpdateByExample="false"
            enableDeleteByExample="false"
            enableSelectByExample="false"
            selectByExampleQueryId="false" >
    	
    </table>

  </context>
</generatorConfiguration>